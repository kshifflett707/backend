{
  "name": "express-babel",
  "version": "1.0.0",
  "description": "",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vmasto/express-babel.git"
  },
  "author": "Kyle Shifflet, Frederick Peavy, William Cory",
  "main": "dist/server/index.js",
  "engines": {
    "node": "~8.9.4",
    "npm": ">=5.8.0"
  },
  "scripts": {
    "prestart": "npm run -s build",
    "start": "node dist/server/index.js",
    "dev": "nodemon src/server/index.js --exec \"node -r dotenv/config -r babel-register\"",
    "clean": "rimraf dist",
    "build": "npm run clean && mkdir -p dist && babel src -s -D -d dist",
    "test": "jest --runinband",
    "resetdb": "sequelize db:drop && sequelize db:create && sequelize db:migrate && sequelize db:seed:all",
    "lint": "esw -w src test"
  },
  "keywords": [
    "express",
    "babel",
    "sequelize",
    "postgresql",
    "jest",
    "eslint"
  ],
  "dependencies": {
    "ansicolor": "^1.1.72",
    "axios": "^0.18.0",
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "bcrypt": "^2.0.1",
    "bluebird": "^3.5.1",
    "body-parser": "^1.18.2",
    "cheerio": "^1.0.0-rc.2",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "dotenv": "^4.0.0",
    "elastic-builder": "^1.1.7",
    "elasticsearch": "^15.0.0",
    "ethereumjs-tx": "^1.3.4",
    "express": "^4.16.2",
    "express-session": "^1.15.6",
    "loglevel": "^1.6.1",
    "morgan": "^1.9.0",
    "ololog": "^1.1.103",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "pg": "^7.4.2",
    "pg-hstore": "^2.3.2",
    "pug": "^2.0.0-beta11",
    "rimraf": "^2.6.2",
    "sequelize": "^4.37.6",
    "sequelize-cli": "^4.0.0",
    "session-file-store": "^1.2.0"
  },
  "devDependencies": {
    "babel-eslint": "^8.0.3",
    "babel-jest": "^21.2.0",
    "babel-register": "^6.26.0",
    "eslint": "^4.12.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jest": "^21.3.2",
    "eslint-watch": "^3.1.3",
    "jest": "^21.2.1",
    "nodemon": "^1.12.1",
    "supertest": "^3.0.0"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ],
    "plugins": [
      "transform-object-rest-spread",
      "transform-class-properties"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "plugins": [
      "import",
      "jest"
    ],
    "parserOptions": {
      "ecmaVersion": 2017,
      "sourceType": "module"
    },
    "env": {
      "node": true,
      "jest": true
    },
    "extends": [
      "eslint:recommended"
    ],
    "rules": {
      "jest/no-focused-tests": 2,
      "jest/no-identical-title": 2
    }
  },
  "jest": {
    "testEnvironment": "node"
  },
  "bugs": {
    "url": "https://github.com/vmasto/express-babel/issues"
  },
  "homepage": "https://github.com/vmasto/express-babel#readme",
  "directories": {
    "test": "test"
  }
}
